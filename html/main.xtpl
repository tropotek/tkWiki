<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    
    <meta name="description" content=""/>
    <meta name="author" content=""/>
    <link rel="icon" href="/favicon.ico"/>
    
    <title></title>

    <!-- TkWiki Theme -->
    <link href="/html/less/tkwiki.less" rel="stylesheet"/>
    
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body>
  
    <div class="navbar-bg navbar-default">
      <div class="container">
        <nav class="navbar navbar-default navbar-fixed-top">
          <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="/" var="siteName" title="Home">
              <img width="25" height="25" alt="Brand" src="/html/images/icons/logo.png" />
            </a>
          </div>

          <div id="navbar" class="collapse navbar-collapse js-navbar-collapse ">
            
            <ul class="nav navbar-nav" var="wiki-menu"></ul>
            
            <ul class="nav navbar-nav navbar-right" choice="logout">
              <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">My Account <span class="caret"></span></a>
                <ul class="dropdown-menu" role="menu">
                  <li class="dropdown-header" var="username"></li>
                  <li><a href="/profile.html">My Profile</a></li>
                  <li><a href="#" var="myPages">My Pages</a></li>
                  <li><a href="/search.html">Search</a></li>
                  <!-- Admin Menu -->
                  <li choice="admin" class="divider"></li>
                  <li choice="admin" class="dropdown-header">Wiki Admin</li>
                  <li choice="admin"><a href="/settings.html">Settings</a></li>
                  <li choice="admin"><a href="/userManager.html">Users</a></li>
                  <li choice="admin"><a href="/pageManager.html">Pages</a></li>
                  <li choice="admin"><a href="/orphaned.html">Orphaned</a></li>
                  <li choice="admin_todo" class="disabled"><a href="#">Plugins</a></li>
                  <li class="divider"></li>
                  <li><a href="/logout.html">Logout</a></li>
                </ul>
              </li>
            </ul>

            <ul class="nav navbar-nav navbar-right" choice="login">
              <li class=""><a href="/login.html" title="Login"><span class="glyphicon glyphicon-log-in"></span> login</a></li>
            </ul>
            
            <form class="navbar-form navbar-right" role="search" choice1="logout" method="post" action="/search.html" id="NavSearch">
              <div class="input-group">
                <input type="text" class="form-control" placeholder="Search" name="search-terms" id="fid-search-terms"  value="" />
                <span class="input-group-btn">
                  <button type="submit" class="btn btn-default" name="search" value="search">Go!</button>
                </span>
              </div>
            </form>
            
            <!-- TODO: Make this a dropdown so it does not impact the search field??? -->
            <form action="/login.html" method="post" class="navbar-form navbar-right" choice="login2" id="NavLogin">
              <div class="form-group">
                <input type="text" name="username" placeholder="Username" class="form-control"/>
              </div>
              <div class="form-group">
                <input type="password" name="password" placeholder="Password" class="form-control"/>
              </div>
              <button type="submit" name="login" value="login" class="btn btn-success">Sign in</button>
              <a class="btn btn-primary" href="/register.html" choice="register">Register</a>
            </form>
            
          </div><!--/.nav-collapse -->
        </nav>
      </div>
    </div>


    <!-- Page Title -->
    <div class="pageWrapper">


      <div class="container">
        <div var="wiki-crumbs"></div>
      </div>
      
      
      
      <div class="container" choice="pageHeading">
        <div class="row">
          <div class="col-md-6">
            <h1 var="pageHeading"></h1>
          </div>
        </div>
      </div>

      <div class="container" choice="alerts">
        <div class="row">
          <div class="col-md-12">
            <div var="alerts"></div>
          </div>
        </div>
      </div>

      <div class="container">
        <div var="content"></div>
      </div>
    </div>


    <footer class="footer">
      <div class="container">
        <hr/>
        <p class="text-muted">Place sticky footer content here.</p>
      </div>
    </footer>


    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="/html/assets/jquery-1.11.3/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="/html/assets/bootstrap-3.3.6/dist/js/bootstrap.min.js"></script>

    <script src="/html/js/jquery-toc/jquery-toc.js"></script>
    <script src="/html/js/jquery-jtable.js"></script>
    
    <script src="/html/assets/tinymce/tinymce.min.js"></script>
    <script src="/html/js/tinymce/plugins/wikilink/wikilink.js"></script>
    <script src="/html/js/tinymce/plugins/wikisave/wikisave.js"></script>
    
    
    <script src="/html/js/tkwiki.js"></script>


  </body>
</html>