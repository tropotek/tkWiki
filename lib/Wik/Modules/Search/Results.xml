<?xml version="1.0" encoding="UTF-8"?>
<div class="SearchResults">

  <div class="resultList">
    <div class="resultHead">
      <h2>
        Search Results for 
        <span var="keywords" />
      </h2>
    </div>

    <div class="resultWrapper">

      <p choice="error" class="warning">No results for your search.</p>

      <div class="resultRow clearfix" repeat="row" var="row">
        <div class="resultContent">
          <h3><a href="#" var="title"></a></h3>
          <div class="resultData" var="content"></div>
          <div class="resultExtra">
            <span var="score" title="Relevance Score" class="score"/> - <a href="#" var="url" class="url"></a> - <span var="size" class="size"/><span choice="lastIndexed"> - <span class="lastindexed" var="lastIndexed" /></span>
          </div>
        </div>
      </div>
      
    </div>
    
    <div class="resultFoot clearfix">
      <div var="Com_Ui_Pager" />
    </div>
  </div>
  
</div>