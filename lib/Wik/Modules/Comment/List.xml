<?xml version="1.0"?>
<div class="Wik_Modules_Comment_List">
  <a name="commentList"></a>
  <p choice="erro3r" class="warningBox"></p>
  
  <div choice="noError" class="results">
    <h3>Comments <a href="javascript:;" var="rssUrl" class="rss" title="Keep up to date on this page's comments with RSS!"></a> </h3>
    
    <div class="resultRow" repeat="row" var="row">
      <div class="head">
        <a name="#" var="anchor" />
        <div class="del" choice="admin"><a href="#" var="delUrl" title="Delete this comment">[ Delete ]</a></div>
        <h4><a href="#" var="titleUrl"></a></h4>
        <div class="created"><span title="created" var="created" /> <em choice="admin"><small var="email"></small> <small var="ip"></small></em></div>
      </div>
      
      <div class="content" var="content"></div>
      
    </div>
    
    <div var="Com_Ui_Pager" />
  </div>
  
</div>