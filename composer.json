{
    "name": "ttek/tkwiki",
    "type": "project",
    "description": "The tkWiki re-borne. V2 has a responsive HTML5 template, new base lib, php 5.4+ optimized, and uses composer to install.",
    "keywords":  [
        "tropotek"
    ],
    "time": "2018-01-15",
    "license": "GPL",
    "version": "2.2.0",
    "minimum-stability": "stable",
    "authors":  [
         {
            "name": "Michael Mifsud",
            "email": "info@tropotek.com.au",
            "homepage": "http://www.tropotek.com/"
        }
    ],
    "autoload":  {
        "psr-0":  {
            "": "src/"
        }
    },
    "scripts":  {
        "post-install-cmd":  [
            "Tk\\Composer\\SetupEvent::postInstall"
        ],
        "post-update-cmd":  [
            "Tk\\Composer\\SetupEvent::postUpdate"
        ]
    },
    "extra":  {
        "branch-alias":  {
            "dev-trunk": "2.2.x-dev"
        }
    },
    "require":  {
        "php": ">=5.3.3",
        "ttek/tk-installers": "~2.0",
        "ttek/tk-framework": "~2.0",
        "ttek/tk-mail": "~2.0",
        "ttek/tk-http": "~2.0",
        "ttek/tk-routing": "~2.0",
        "ttek/tk-auth": "~2.0",
        "ttek/tk-database": "~2.0",
        "ttek/tk-mvc": "~2.0",
        "ttek/tk-domtemplate": "~2.0",
        "ttek/tk-form": "~2.0",
        "ttek/tk-table": "~2.0",
        "ttek/tk-plugin": "~2.0",
        "ttek/tk-ui": "~2.0"
    },
    "require-dev":  {
    }
}