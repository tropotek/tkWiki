{
    "name": "ttek/tkwiki",
    "type": "project",
    "description": "The tkWiki re-borne. V2 has a responsive HTML5 template, new base lib, php 5.4+ optimized, and uses composer to install.",
    "keywords":  [
        "tropotek"
    ],
    "time": "2014-10-31",
    "license": "GPL",
    "version": "master",
    "minimum-stability": "dev",
    "authors":  [
         {
            "name": "Michael Mifsud",
            "email": "info@tropotek.com.au",
            "homepage": "http://www.tropotek.com/"
        }
    ],
    "autoload":  {
        "psr-0":  {
            "": "src/"
        },
        "files":  [
            "src/App/Bootstrap.php"
        ]
    },
    "scripts":  {
        "post-install-cmd":  [
            "Ts\\Composer\\InitProjectEvent::postInstall"
        ],
        "post-update-cmd":  [
            "Ts\\Composer\\InitProjectEvent::postUpdate"
        ]
    },
    "extra":  {
        "branch-alias":  {
            "dev-trunk": "2.0.x-dev"
        }
    },
    "require":  {
        "php": ">=5.0.0",
        "ttek/tk-installers": "~2.0",
        "ttek/tk-framework": "~2.0",
        "ttek/tk-domtemplate": "~2.0",
        "ttek/tk-http": "~2.0",
        "ttek/tk-routing": "~2.0",
        "ttek/tk-auth": "~2.0",
        "ttek/tk-database": "~2.0",
        "ttek/tk-mvc": "~2.0",
        "ttek/tk-form": "~2.0",
        "ttek/tk-table": "~2.0",
        "ttek/tk-site": "~2.0",
        "ttek/tk-mail": "~2.0",
        "ttek/tk-plugin": "~2.0",
        "monolog/monolog": "^1.13@dev",
        "ttek-plg/example": "~1.0"
    },
    "require-dev":  {
    }
}